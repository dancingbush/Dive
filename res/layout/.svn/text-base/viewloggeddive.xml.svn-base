<?xml version="1.0" encoding="utf-8"?>

<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content" >
    
<LinearLayout 
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >
    
<LinearLayout 
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_margin="10dp"
   
    android:orientation="horizontal">
    <TextView
        android:id="@+id/tv_diveNumberLogged"
        android:layout_width="wrap_content"
    	android:layout_height="wrap_content"
    	
        android:text="Logged Dive Details:"
        android:textColor="#FF0000"
        android:typeface="sans"
        android:textSize="15dip"
        android:textStyle="bold" 
        android:layout_weight="50"></TextView>
    <EditText
        android:id="@+id/et_diveNumberLogged"
        android:inputType="number"
        android:layout_width="wrap_content"
    	android:layout_height="wrap_content"
        android:hint="0"
        android:textColor="#FF0000"
        android:typeface="sans"
        android:textSize="15dip"
        android:textStyle="bold" 
        android:layout_weight="50" >
        
    </EditText>
       </LinearLayout>
       
     <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="10dp" >

    <EditText
        android:id="@+id/et_locationLogDiveLogged"
        android:layout_toRightOf="@+id/ib_logDiveImage"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:hint="Get Location"
        android:ems="10" >

        <requestFocus />

    </EditText>
<!-- click this to call map activity -->
     <ImageButton
        android:id="@+id/ib_findLocationOnMapLogged"
        android:layout_width="45dip"
        android:layout_height="45dip"
        android:layout_toRightOf="@+id/et_locationLogDive"
      
        android:src="@drawable/searchmap" />
     
    <EditText
        android:id="@+id/et_diveSiteLogDive_Logged"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:paddingTop="10dp"
        
        android:layout_below="@+id/et_locationLogDive"
        android:layout_toRightOf="@+id/ib_logDiveImage"
        android:ems="10"
        android:hint="Dive Site" />

    <EditText
        android:id="@+id/dp_diveDate_Logged"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_below="@+id/et_diveSiteLogDive"
        android:ems="10"
        android:hint="Enter Date or Dbl Click"
        
        android:calendarViewShown="false" />

    <ImageButton
        android:id="@+id/ib_logDiveImage_Logged"
        android:layout_width="145dp"
        android:layout_height="110dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:src="@drawable/camera1" />

    </RelativeLayout>

     <RatingBar
         android:id="@+id/diveratingBar1_Logged"
         android:layout_width="wrap_content"
         android:layout_height="wrap_content" 
         android:rating="2"/>
    
     <!-- Water Temp -->
    <LinearLayout 
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_margin="10dp"
   
    android:orientation="horizontal">
    <TextView
        android:id="@+id/tv_diveTemp_Logged"
        android:layout_width="wrap_content"
    	android:layout_height="wrap_content"
    	
        android:text="Water Temp (c)"
        android:textColor="#33FF00"
        android:typeface="sans"
        android:textSize="15dip"
        android:textStyle="bold" 
        android:layout_weight="50"></TextView>
    <EditText
        android:id="@+id/et_waterTemp_Logged"
        android:inputType="number"
        android:layout_width="wrap_content"
    	android:layout_height="wrap_content"
        android:hint="0"
        android:textColor="#FF0000"
        android:typeface="sans"
        android:textSize="15dip"
        android:textStyle="bold" 
        android:layout_weight="50" >
        
    </EditText>
       </LinearLayout>
    
    <!-- visibilty -->
    <LinearLayout 
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_margin="10dp"
   
    android:orientation="horizontal">
    <TextView
        android:id="@+id/tv_visibilty_Logged"
        android:layout_width="wrap_content"
    	android:layout_height="wrap_content"
    	
        android:text="Visibilty(m)"
        android:textColor="#33FF00"
        android:typeface="sans"
        android:textSize="15dip"
        android:textStyle="bold" 
        android:layout_weight="50"></TextView>
    <EditText
        android:id="@+id/et_visibilty_Logged"
        android:inputType="number"
        android:layout_width="wrap_content"
    	android:layout_height="wrap_content"
        android:hint="0"
        android:textColor="#FF0000"
        android:typeface="sans"
        android:textSize="15dip"
        android:textStyle="bold" 
        android:layout_weight="50" >
        
    </EditText>
       </LinearLayout>
 
    
    
    <!-- Start and End pressuers -->
     <LinearLayout 
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_margin="10dp"
   
    android:orientation="horizontal">
    <TextView
        android:id="@+id/tv_startPressure_Logged"
        android:layout_width="wrap_content"
    	android:layout_height="wrap_content"
    	
        android:text="Start Pressure (Bar)"
        android:textColor="#33FF00"
        android:typeface="sans"
        android:textSize="15dip"
        android:textStyle="bold" 
        android:layout_weight="50"></TextView>
    <EditText
        android:id="@+id/et_startPressure_Logged"
        android:inputType="number"
        android:layout_width="wrap_content"
    	android:layout_height="wrap_content"
        android:hint="0"
        android:textColor="#FF0000"
        android:typeface="sans"
        android:textSize="15dip"
        android:textStyle="bold" 
        android:layout_weight="50" >
        
    </EditText>
       </LinearLayout>
    
    <LinearLayout 
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_margin="10dp"
   
    android:orientation="horizontal">
    <TextView
        android:id="@+id/tv_endPressure_Logged"
        android:layout_width="wrap_content"
    	android:layout_height="wrap_content"
    	
        android:text="End Pressure (Bar)"
        android:textColor="#33FF00"
        android:typeface="sans"
        android:textSize="15dip"
        android:textStyle="bold" 
        android:layout_weight="50"></TextView>
    <EditText
        android:id="@+id/et_endPressure_Logged"
        android:inputType="number"
        android:layout_width="wrap_content"
    	android:layout_height="wrap_content"
        android:hint="0"
        android:textColor="#FF0000"
        android:typeface="sans"
        android:textSize="15dip"
        android:textStyle="bold" 
        android:layout_weight="50" >
        
    </EditText>
       </LinearLayout>
    <!-- Conditions -->
    <LinearLayout 
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_margin="10dp"
   
    android:orientation="horizontal">
    <TextView
        android:id="@+id/tv_diveConditions_Logged"
        android:layout_width="wrap_content"
    	android:layout_height="wrap_content"
    	
        android:text="Dive Conditions"
        android:textColor="#FF00FF"
        android:typeface="sans"
        android:textSize="15dip"
        android:textStyle="bold" 
        android:layout_weight="50"></TextView>
     <Spinner
        android:id="@+id/spinner_diveConditons_Logged"
        android:layout_width="135dp"
        android:layout_height="wrap_content"
        
        android:entries="@array/diveConditions_arrays"
        />
       </LinearLayout>
       
    <!-- Bottom Time -->
    <LinearLayout 
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_margin="10dp"
   
    android:orientation="horizontal">
    <TextView
        android:id="@+id/tv_bottomTime_Logged"
        android:layout_width="wrap_content"
    	android:layout_height="wrap_content"
    	
        android:text="Bottom Time (mins)"
        android:textColor="#33FF00"
        android:typeface="sans"
        android:textSize="15dip"
        android:textStyle="bold" 
        android:layout_weight="50"></TextView>
    <EditText
        android:id="@+id/et_bottomTime_Logged"
        android:inputType="number"
        android:layout_width="wrap_content"
    	android:layout_height="wrap_content"
        android:hint="0"
        android:textColor="#FF0000"
        android:typeface="sans"
        android:textSize="15dip"
        android:textStyle="bold" 
        android:layout_weight="50" >
        
    </EditText>
       </LinearLayout>
       
    <!-- Dive Buddy -->
    <LinearLayout 
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_margin="10dp"
   
    android:orientation="horizontal">
    <TextView
        android:id="@+id/tv_diveBuddy_Logged"
        android:layout_width="wrap_content"
    	android:layout_height="wrap_content"
    	
        android:text="Dive Buddy"
        android:textColor="#33FF00"
        android:typeface="sans"
        android:textSize="15dip"
        android:textStyle="bold" 
        android:layout_weight="50"></TextView>
    <EditText
        android:id="@+id/et_diveBuddy_Logged"
        
        android:layout_width="wrap_content"
    	android:layout_height="wrap_content"
        android:hint="Bar"
        android:textColor="#FF0000"
        android:typeface="sans"
        android:textSize="15dip"
        android:textStyle="bold" 
        android:layout_weight="50" >
        
    </EditText>
       </LinearLayout>
       
    <!-- Dive Center -->
    <!-- Dive Buddy -->
    <LinearLayout 
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_margin="10dp"
   
    android:orientation="horizontal">
    <TextView
        android:id="@+id/tv_diveCenter_Logged"
        android:layout_width="wrap_content"
    	android:layout_height="wrap_content"
    	
        android:text="Dive Center"
        android:textColor="#33FF00"
        android:typeface="sans"
        android:textSize="15dip"
        android:textStyle="bold" 
        android:layout_weight="50"></TextView>
    <EditText
        android:id="@+id/et_diveCenter_Logged"
        
        android:layout_width="wrap_content"
    	android:layout_height="wrap_content"
        android:hint="Center"
        android:textColor="#FF0000"
        android:typeface="sans"
        android:textSize="15dip"
        android:textStyle="bold" 
        android:layout_weight="50" >
        
    </EditText>
       </LinearLayout>
    <EditText 
        android:id="@+id/et_DiveComments_Logged"
        android:hint="Comments"
       
        android:layout_width="fill_parent"
        android:layout_height="120dp"
        />
    
      <RelativeLayout  android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="10dp">
    <Button 
        android:layout_width="125dip"
        android:layout_height="wrap_content"
        android:layout_margin="10dp"
        android:id="@+id/button_SaveDiveLogEntry_Logged"
        android:background="@drawable/list_selectors"
        
        android:text="Save"
        android:textColor="#000033"
        android:typeface="serif"
        
        />
    <Button 
         android:layout_width="125dip"
        android:layout_height="wrap_content"
        android:layout_toRightOf="@+id/button_SaveDiveLogEntry_Logged"
        android:layout_margin="10dp"
        android:id="@+id/button_ViewSavedDive"
        android:background="@drawable/list_selectors"
        
        android:text="View"
        android:textColor="#000033"
        android:typeface="serif"
        
        
        />
    
    </RelativeLayout>
</LinearLayout>


</ScrollView>
